{
  "name": "@engagespot/core",
  "version": "0.1.1-alpha.13",
  "description": "Engagespot Javascript Core Functions",
  "source": "./src/index.ts",
  "main": "./dist/engagespot-core.js",
  "module": "./dist/engagespot-core.esm.js",
  "unpkg": "./dist/engagespot-core.umd.js",
  "types": "./dist/engagespot-core.d.ts",
  "amdName": "EngagespotCore",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "yarn clean && yarn tsc && microbundle && yarn declarations",
    "dev": "microbundle watch",
    "tsc": "tsc",
    "clean": "rimraf dist",
    "declarations": "api-extractor run --local --verbose",
    "docs": "api-documenter markdown --input-folder spec --output ./spec/markdown",
    "prepare": "yarn run build",
    "test:dev": "jest --watchAll",
    "test": "jest"
  },
  "keywords": [
    "engagespot",
    "client",
    "javascript",
    "notifications"
  ],
  "author": "Engagespot",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.13.77",
    "@microsoft/api-extractor": "^7.19.2",
    "@testing-library/dom": "^8.11.1",
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "microbundle": "0.14.2",
    "msw": "^0.36.3",
    "rimraf": "^3.0.2",
    "ts-jest": "27.1.2",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.11.11",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "ably": "^1.2.15"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslint.rc.js"
    ]
  }
}
