{
  "name": "@engagespot/react-hooks",
  "version": "0.1.1-alpha.3",
  "source": "./src/index.ts",
  "main": "./dist/engagespot-hooks.js",
  "module": "./dist/engagespot-hooks.esm.js",
  "unpkg": "./dist/engagespot-hooks.umd.js",
  "types": "./dist/engagespot-hooks.d.ts",
  "amdName": "EngagespotHooks",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "yarn clean && yarn tsc && microbundle --jsx React.createElement && yarn declarations",
    "dev": "microbundle watch --jsx React.createElement",
    "tsc": "tsc",
    "declarations": "api-extractor run --local --verbose",
    "clean": "rimraf dist",
    "prepare": "yarn run build",
    "test:dev": "jest --watchAll",
    "test": "cross-env CI=true jest",
    "lint:css": "stylelint './src/**/*.js'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^16.8.3 || ^17.0.0-0"
  },
  "dependencies": {
    "@engagespot/core": "^0.1.1-alpha.3",
    "@popperjs/core": "^2.9.1",
    "date-fns": "2.27.0",
    "intersection-observer": "0.12.0",
    "lodash.merge": "4.6.2",
    "react-popper": "^2.2.5"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.13.77",
    "@microsoft/api-extractor": "7.19.2",
    "@types/react": "17.0.37",
    "microbundle": "0.14.2",
    "react": "17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "26.5.3",
    "typescript": "4.2.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
