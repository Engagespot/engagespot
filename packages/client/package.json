{
  "name": "@engagespot/client",
  "version": "0.1.12-alpha.2",
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "license": "MIT",
  "keywords": [
    "engagespot",
    "notification feed",
    "notification inbox",
    "in-app notifications"
  ],
  "scripts": {
    "build": "yarn tsc && yarn build:package && yarn build:cdn",
    "build:package": "tsup",
    "build:cdn": "tsup --config ./tsup.cdn.config.ts",
    "dev": "tsup --minify false --sourcemap inline",
    "tsc": "tsc",
    "clean": "rimraf dist",
    "declarations": "api-extractor run --local --verbose",
    "docs": "api-documenter markdown --input-folder spec --output ./spec/markdown",
    "prepare": "tsup",
    "test:dev": "jest --watchAll",
    "test": "jest",
    "lint:css": "stylelint './src/**/*.js'"
  },
  "dependencies": {
    "@engagespot/react-component": "^0.1.12-alpha.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.13.77",
    "@microsoft/api-extractor": "^7.19.2",
    "@types/jest": "^27.0.3",
    "@types/lodash.mapkeys": "4.6.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.throttle": "4.1.6",
    "@types/react-transition-group": "4.4.4",
    "@types/styled-components": "5.1.15",
    "axios": "^0.24.0",
    "babel-loader": "^8.2.3",
    "cross-env": "7.0.3",
    "dotenv": "^16.0.1",
    "jest": "^27.4.5",
    "microbundle": "0.14.2",
    "msw": "^0.36.3",
    "node-fetch": "^3.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "27.1.2",
    "tslib": "2.3.1",
    "tsup": "^6.1.2",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.11.11",
    "typescript": "^4.5.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslint.rc.js"
    ]
